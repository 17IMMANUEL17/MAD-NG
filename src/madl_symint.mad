     corr(elm, mflw, lwn , sabac4_c[1]) mflw.npos = 'first'
    thick(elm, mflw, lwn * sabac4_d[1]) mflw.npos = nil
     thin(elm, mflw, lwn * sabac4_k[1])
    thick(elm, mflw, lwn * sabac4_d[2])
     thin(elm, mflw, lwn * sabac4_k[2])
