-- Yoshida schemes ------------------------------------------------------------o

-- [Yoshida90] eq 5.18 and p267
local function make_yoshida (w)
  local d, k, n, s = {}, {}, #w+1, 0
  for i=1,n-1 do k[i] = w[n-i] ; s = s+w[i] end
  d[1], k[n] = k[1]/2, 1-2*s
  for i=2,n do d[i] = (k[i]+k[i-1])/2 end
  -- local printf in MAD.utility
  -- for i=1,#d do
  --  printf("%d, % -.10f, % -.10f", i, d[i], k[i])
  -- end
  return d, k
end

