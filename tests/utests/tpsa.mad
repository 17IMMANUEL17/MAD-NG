    assertTrue((mkt(idx[2],idx[4]-1)*mkt(idx[2],idx[4]-1):inv()) == 1)
    assertTrue((mkt(idx[3],idx[5]-1)*mkt(idx[3],idx[5]-1):inv()) == 1)
  end
end


