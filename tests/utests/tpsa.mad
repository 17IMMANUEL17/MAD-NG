function TestTPSAArithmetic:testMulC()
local ct,cv = ctpsa, cvector

  for _,d in ipairs({d1v,d2v,d3v,d5v}) do
    local mo, nc = d:maxord(), d:maxlen()
    local idx = {[0]=1} for o=0,mo do idx[o+1] = d:maxlen(o)+1 end

    -- helpers to setvec tpsas and vectors within start..end
    local mkt = \s,e -> ct(d):setvec(s, cv(e-s+1):fill((s..e))*1i)

    assertTrue((mkt(idx[0],idx[1]-1)*mkt(idx[0],idx[1]-1)):getvec(1,nc) == polymul(mkt(idx[0],idx[1]-1),mkt(idx[0],idx[1]-1),d,d,ct))
    assertTrue((mkt(idx[1],idx[2]-1)*mkt(idx[0],idx[1]-1)):getvec(1,nc) == polymul(mkt(idx[1],idx[2]-1),mkt(idx[0],idx[1]-1),d,d,ct))
    assertTrue((mkt(idx[0],idx[1]-1)*mkt(idx[1],idx[2]-1)):getvec(1,nc) == polymul(mkt(idx[0],idx[1]-1),mkt(idx[1],idx[2]-1),d,d,ct))
    assertTrue((mkt(idx[1],idx[3]-1)*mkt(idx[0],idx[1]-1)):getvec(1,nc) == polymul(mkt(idx[1],idx[3]-1),mkt(idx[0],idx[1]-1),d,d,ct))
