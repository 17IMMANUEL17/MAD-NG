function TestUnaryMinus:testminusSingVar()

  local t1v = tpsa(d1v)
  local ct1v = ctpsa(d1v)
  local t_temp = tpsa(d1v)
  local ct_temp = ctpsa(d1v)

  t1v:set(1,1)
  t1v:set(3,1)
  t1v:set(5,1)
  t_temp:set(1,-1)
  t_temp:set(3,-1)
  t_temp:set(5,-1)
  ct1v:set(1,1 + 1i)
  ct1v:set(3,1 + 1i)
  ct1v:set(5,1 + 1i)
  ct_temp:set(1,-1 - 1i)
  ct_temp:set(3,-1 - 1i)
  ct_temp:set(5,-1 - 1i)

  assertTrue(t1v == -t_temp)
  assertTrue(-ct_temp == ct1v)

  --clean the tpsa for the other tests
  t1v:set(1,0)
  t1v:set(3,0)
  t1v:set(5,0)
  ct1v:set(1,0)
  ct1v:set(3,0)
  ct1v:set(5,0)

end


