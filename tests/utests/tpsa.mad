local function multiply_polynomials(P, Q)
    local result = {}
    for exp1_mono, coeff1 in pairs(P) do        
        for exp2_mono, coeff2 in pairs(Q) do                
            local new_exp = exp1_mono + exp2_mono
            local new_coeff = coeff1 * coeff2
            if result[tostring(new_exp)] then
                result[tostring(new_exp)] = result[tostring(new_exp)] + new_coeff
            else
                result[tostring(new_exp)] = new_coeff
            end
        end
    end
  return result
end


local function tpsa2Poly(t)
  local poly = {}
  local size = t:maxlen()
  for i=1,size do
    local mono,_ = t:get_mono(i)
    poly[mono] = t:get(i)
  end
  return poly
end

local function poly2Vec(poly,d,ctor)
  local t    = ctor(d)
  local size = d:maxlen()
  for exp, coeff in pairs(poly) do
    if d:get_idx(exp)~=0 then t:set(exp,coeff) end
  end
  --t:print()
  return t:getvec(1,size)
end

local function polymul(t1,t2,d1,d2,ctor) 
  local P,Q = tpsa2Poly(t1,d1),tpsa2Poly(t2,d2)
  local r =  multiply_polynomials(P, Q)
  return poly2Vec(r,d1,ctor) --to be changed, descriptor not correct
end


