function TestCloning:testCopySingVar()

  local t1v = tpsa(d1v)
  local ct1v = ctpsa(d1v)
  local t_temp = tpsa(d1v)
  local ct_temp = ctpsa(d1v)

  t1v:set(1,1)
  t1v:set(3,1)
  t1v:set(5,1)
  ct1v:set(1,1)
  ct1v:set(3,1)
  ct1v:set(5,1)
  
  t1v:copy(t_temp)
  ct1v:copy(ct_temp)

  assertTrue(t1v==t_temp)
  assertTrue(ct1v==ct_temp)

  t1v:set(1,0)
  ct1v:set(1,0)
  
  t_temp = t1v:copy()
  ct_temp = ct1v:copy()
  assertTrue(t1v==t_temp)
  assertTrue(ct1v==ct_temp)

  --clean the tpsa for the other tests
  t1v:set(1,0)
  t1v:set(3,0)
  t1v:set(5,0)
  ct1v:set(1,0)
  ct1v:set(3,0)
  ct1v:set(5,0)

end


