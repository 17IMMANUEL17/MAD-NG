end


function TestTPSASet:testSetVecMultVar()
  local sz_t = d3v:maxlen()
  local t    = tpsa (d3v):setvec(5,1..sz_t-25)   --filling all the orders but the first and last ones
  local ct   = ctpsa(d3v):setvec(5,1..sz_t-25)*1i
  local vec1 = vector(35):fill(0..    34):concat(vector(20):zeros(), "vec")
  local vec2 = vector( 4):fill({1,2,3,4}):concat(vector(19):zeros(), "vec")
  local vec3 = vector( 4):fill({1,2,3,4}):concat(vector(17):zeros(), "vec")
  local vec4 = vector( 3):zeros():concat(vector(4):fill({1,2,3,4}):concat(vector(14):zeros(),"vec"),"vec")

  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec( 2, 1.. 3):getvec(2,  3), vector( 3):fill({1,2,3    }))
  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec( 3, 1.. 1):getvec(2,  3), vector( 3):fill({0,1,0    }))
  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec( 3, 1.. 2):getvec(2,  3), vector( 3):fill({0,1,2    }))
  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec( 3, 1.. 4):getvec(2,  5), vector( 5):fill({0,1,2,3,4}))
  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec(19, 1.. 4):getvec(19, 4), vector( 4):fill({1,2,3,4  }))
  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec(36, 1..21):getvec(36,21), vector(21):fill(   1..21   ))
  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec( 3, 1..34):getvec(2, 55), vec1)
  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec( 3, 1..34):getvec(2, 55), vec1)
  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec(34, 1.. 4):getvec(34,23), vec2)
  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec(36, 1.. 4):getvec(36,21), vec3)
  assertEquals(tpsa (d3v):setvec(5,1..sz_t-25):setvec(39, 1.. 4):getvec(36,21), vec4)
  assertError(t .getvec,      0, 1)
  assertError(ct.getvec,      0, 1)
  assertError(t .getvec, sz_t+1, 1)
  assertError(ct.getvec, sz_t+1, 1)
end


function TestTPSALinComb:testTPSALinCombSingVar()
  local t_scalar  =  tpsa(d1v):set("0",1)
  local t_ord1    =  tpsa(d1v):set("1",2)
  local t_ord12    =  tpsa(d1v):set("1",2):set("2",3)
  local t_ord15    =  tpsa(d1v):setvec(2, {2,3,4,5,6})
  local t_ord24    =  tpsa(d1v):setvec(3, {3,4,5})
  local t_ord45    =  tpsa(d1v):setvec(5, {5,6})
  local t_ord5    =  tpsa(d1v):set("5", 6)

  assertTrue(t_scalar:axpbypc(t_scalar,2,3,4) == tpsa(d1v):setvec(1,{9}))
  assertTrue(t_ord1:axpbypc(t_scalar,2,3,4) == tpsa(d1v):setvec(1,{7,4}))
  assertTrue(t_scalar:axpbypc(t_ord1,3,2,4) == tpsa(d1v):setvec(1,{7,4}))
  assertTrue(t_ord12:axpbypc(t_scalar,2,3,4) == tpsa(d1v):setvec(1,{7,4,6}))
  assertTrue(t_scalar:axpbypc(t_ord12,3,2,4) == tpsa(d1v):setvec(1,{7,4,6}))
  assertTrue(t_ord15:axpbypc(t_scalar,2,3,4) == tpsa(d1v):setvec(1,{7,4,6,8,10,12}))
