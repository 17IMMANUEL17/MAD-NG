  assertEquals(ctpsa(d1v  ):setvec(1,{1        }):getvec(1,6), vector(6):fill({1,0,0,0,0,0}))
  assertEquals(ctpsa(d1v  ):setvec(1,{1,1      }):getvec(1,6), vector(6):fill({1,1,0,0,0,0}))
  assertEquals(ctpsa(d1v  ):setvec(2,{1,1      }):getvec(1,6), vector(6):fill({0,1,1,0,0,0}))
