function TestTPSAArithmetic:testSubR()
  local t,v =tpsa, vector
  
  -- helpes to set tpsas within start..end
  local mkt = \s,e -> t():setvec(s, v(e-s+1):fill((s..e)))

  for _,d in ipairs({d1v,d2v,d3v,d5v}) do
    local mo, nc = d:maxord(), d:maxlen()
    local idx = {[0]=1} for o=0,mo do idx[o+1] = d:maxlen(o)+1 end

      -- helpers to setvec tpsas and vectors within start..end
      local mkt = \s,e -> t(d):setvec(s, v(e-s+1):fill((s..e)))

    assertTrue(mkt(idx[0],idx[1]-1):axpbypc(mkt(idx[0],idx[1]-1),1,-1,0) == mkt(idx[0],idx[1]-1)-mkt(idx[0],idx[1]-1))
    assertTrue(mkt(idx[1],idx[2]-1):axpbypc(mkt(idx[0],idx[1]-1),1,-1,0) == mkt(idx[1],idx[2]-1)-mkt(idx[0],idx[1]-1))
    assertTrue(mkt(idx[0],idx[1]-1):axpbypc(mkt(idx[1],idx[2]-1),1,-1,0) == mkt(idx[0],idx[1]-1)-mkt(idx[1],idx[2]-1))
    assertTrue(mkt(idx[1],idx[3]-1):axpbypc(mkt(idx[0],idx[1]-1),1,-1,0) == mkt(idx[1],idx[3]-1)-mkt(idx[0],idx[1]-1))
    assertTrue(mkt(idx[0],idx[1]-1):axpbypc(mkt(idx[1],idx[3]-1),1,-1,0) == mkt(idx[0],idx[1]-1)-mkt(idx[1],idx[3]-1))
    assertTrue(mkt(idx[1],idx[5]-1):axpbypc(mkt(idx[0],idx[1]-1),1,-1,0) == mkt(idx[1],idx[5]-1)-mkt(idx[0],idx[1]-1))
    assertTrue(mkt(idx[0],idx[1]-1):axpbypc(mkt(idx[1],idx[5]-1),1,-1,0) == mkt(idx[0],idx[1]-1)-mkt(idx[1],idx[5]-1))
    assertTrue(mkt(idx[4],idx[5]-1):axpbypc(mkt(idx[0],idx[1]-1),1,-1,0) == mkt(idx[4],idx[5]-1)-mkt(idx[0],idx[1]-1))
    assertTrue(mkt(idx[0],idx[1]-1):axpbypc(mkt(idx[4],idx[5]-1),1,-1,0) == mkt(idx[0],idx[1]-1)-mkt(idx[4],idx[5]-1))
    assertTrue(mkt(idx[1],idx[2]-1):axpbypc(mkt(idx[1],idx[2]-1),1,-1,0) == mkt(idx[1],idx[2]-1)-mkt(idx[1],idx[2]-1))
    assertTrue(mkt(idx[1],idx[3]-1):axpbypc(mkt(idx[1],idx[2]-1),1,-1,0) == mkt(idx[1],idx[3]-1)-mkt(idx[1],idx[2]-1))
    assertTrue(mkt(idx[1],idx[2]-1):axpbypc(mkt(idx[1],idx[3]-1),1,-1,0) == mkt(idx[1],idx[2]-1)-mkt(idx[1],idx[3]-1))
    assertTrue(mkt(idx[2],idx[4]-1):axpbypc(mkt(idx[1],idx[2]-1),1,-1,0) == mkt(idx[2],idx[4]-1)-mkt(idx[1],idx[2]-1))
    assertTrue(mkt(idx[1],idx[2]-1):axpbypc(mkt(idx[2],idx[4]-1),1,-1,0) == mkt(idx[1],idx[2]-1)-mkt(idx[2],idx[4]-1))
    assertTrue(mkt(idx[3],idx[5]-1):axpbypc(mkt(idx[1],idx[2]-1),1,-1,0) == mkt(idx[3],idx[5]-1)-mkt(idx[1],idx[2]-1))
