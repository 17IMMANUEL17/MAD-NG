function TestTPSASet:testSetVec ()
  local t,v = tpsa, vector
  local mo, nc = d5v:maxord(), d5v:maxlen()
  local idx = {[0]=0} for o=0,mo do idx[o+1] = d5v:maxlen(o)+1 end

