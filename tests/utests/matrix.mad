function TestMatrixInPlaceII:testPerm()
  local oldV = {}
  local fact = \n => for i=1,n-1 do n=n*i end return n end
  for j=1,5 do
    local v = vector(j):fill(1..j)
    for i=1,fact(j) do
      v = v:perm()
      assertFalse( v == oldV[i] )
      for is=1,v:size() do
        assertTrue( v:geti(is) <= j )
        assertTrue( v:geti(is) >= 1 )
      end
      oldV[i] = v
    end
  end
end

