function TestMatrixFun:testMapat()
  local m1
  local function cont(tbl, val)
     for i=1,#tbl do
        if tbl[i] == val then
           return true
        end
     end
     return false
  end

  for i,m in ipairs(self.mat) do
    local nr, nc = m:sizes()
    for i=1,m:size() do
      m1 = m:mapat('~', i)
      for ii=1,#m1 do if ii==i then assertEquals( m1:geti(ii),-ii )
                               else assertEquals( m1:geti(ii), ii ) end end

      for a=1,i do
      for li=1,i-a+1 do
        local ra = a..i..li
        local ir = { ra, totable(ra), vector((totable(ra))) }
        for d=1,#ir do
          m1 = m:mapat('~', ir[d])
          for ii=1,#m1 do
            if cont(totable(ra),ii) then assertEquals( m1:geti(ii),-ii )
                                    else assertEquals( m1:geti(ii), ii ) end
          end

          --print(m1)
          m1 = m:mapat('~', ir[d], {})
          --print(m1, "\n")
          for ii=1,#m1 do
            --if cont(totable(ra),ii) then print( m1[ii],-ii )
            --              else print( m1[ii], ii ) end
          end
        end
      end end
    end
  end
end

