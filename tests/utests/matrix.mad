            else assertEquals( mt:get(i,j), 0 ) end
  end end
  mt:setsub( 1..5, 1, 1..5)
  for i=1,5 do
  for j=1,5 do
    if j==1 then assertEquals( mt:get(i,j), i )
            else assertEquals( mt:get(i,j), 0 ) end
  end end
