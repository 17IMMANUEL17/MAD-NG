#== initialization =============================================================
if (!(exist("MAD_MIN") && exist("MAD_MAX"))) {
  MAD_MIN = 1000000;
  MAD_MAX = -1;
}
MAD_RESET_OBJECT = "unset for [i=MAD_MIN:MAD_MAX] object i"
MAD_RESET_LABEL  = "unset for [i=MAD_MIN:MAD_MAX] label i"
if (!(exist("MAD_OLD_TERM") && exist("MAD_NEW_TERM"))) {
  MAD_OLD_TERM = "";
  MAD_NEW_TERM = "";
}
MAD_SET_TERM = "if (!(MAD_OLD_TERM eq MAD_NEW_TERM)) { \
  @MAD_NEW_TERM; \
  MAD_OLD_TERM = MAD_NEW_TERM; \
}"
#===============================================================================

@MAD_SET_TERM
@MAD_RESET_OBJECT
@MAD_RESET_LABEL
set grid 
set border 15
set title 'TestPlotScript.testPlotDataStyleTypeSpec1' font ','
set key left top font ','
set size 7.00000e-01, 6.00000e-01
set origin 3.00000e-01, 4.00000e-01
$MAD_DATA << EOD
