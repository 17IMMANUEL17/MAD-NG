#== initialization =============================================================
if (!exist("MAD_MIN")) {
  MAD_MIN      = 1000000;
  MAD_MAX      = -1;
  MAD_WID      = -1;
  MAD_TERM     = "";
  MAD_OLD_WID  = -1;
  MAD_OLD_TERM = "";
}
MAD_INC_WID      = "MAD_WID = MAD_WID + 1"
MAD_RESET_OBJECT = "unset for [i=MAD_MIN:MAD_MAX] object i"
MAD_RESET_LABEL  = "unset for [i=MAD_MIN:MAD_MAX] label i"
MAD_SET_TERM     = " \
  if (!((MAD_OLD_TERM eq MAD_TERM) && \
        (MAD_OLD_WID  == MAD_WID ))) { \
    @MAD_TERM; \
    MAD_OLD_TERM = MAD_TERM; \
    MAD_OLD_WID  = MAD_WID; \
  } \
"
#===============================================================================

@MAD_SET_TERM
@MAD_RESET_OBJECT
@MAD_RESET_LABEL
set xtics nomirror
set x2tics nomirror
set ytics nomirror
set y2tics nomirror
set grid 
set border 15
set title 'TestPlotScript.testAxisSelection4' font ','
set key left top font ','
