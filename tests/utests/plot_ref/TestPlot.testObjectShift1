#== initialization =============================================================
if (!(exist("MAD_MIN") && exist("MAD_MAX"))) {
  MAD_MIN = 11;
  MAD_MAX = -1;
}
MAD_RESET_OBJECT = "unset for [i=MAD_MIN:MAD_MAX] object i"
MAD_RESET_LABEL  = "unset for [i=MAD_MIN:MAD_MAX] label i"
if (!(exist("MAD_OLD_TERM") && exist("MAD_NEW_TERM"))) {
  MAD_OLD_TERM = "";
  MAD_NEW_TERM = "";
}
MAD_SET_TERM = "if (!(MAD_OLD_TERM eq MAD_NEW_TERM)) { \
  @MAD_NEW_TERM; \
  MAD_OLD_TERM = MAD_NEW_TERM; \
}"
#===============================================================================

@MAD_SET_TERM
@MAD_RESET_OBJECT
@MAD_RESET_LABEL
set object 11 polygon front dashtype solid \
from first 0.00000e+00, screen 5.89679e-01 to first 1.00000e+01, screen 5.89679e-01 to first 0.00000e+00, screen 5.89679e-01
set label 12 back at 0.00000e+00, screen 5.89679e-01 '$start [marker]' hypertext point pt 4 ps 1 lc 'white'
set object 12 polygon front fc rgb 'white' fs solid 1.0 border rgb 'black'  \
  from 0.00000e+00, screen 2.04358e-01 \
  to 0.00000e+00, screen 9.75000e-01 \
  to 0.00000e+00, screen 2.04358e-01

set label 13 back at 5.00000e+00, screen 5.89679e-01 'middle [marker]' hypertext point pt 4 ps 1 lc 'white'
set object 13 polygon front fc rgb 'white' fs solid 1.0 border rgb 'black'  \
  from 5.00000e+00, screen 2.04358e-01 \
  to 5.00000e+00, screen 9.75000e-01 \
  to 5.00000e+00, screen 2.04358e-01

set label 14 back at 1.00000e+01, screen 5.89679e-01 '$end [marker]' hypertext point pt 4 ps 1 lc 'white'
set object 14 polygon front fc rgb 'white' fs solid 1.0 border rgb 'black'  \
  from 1.00000e+01, screen 2.04358e-01 \
  to 1.00000e+01, screen 9.75000e-01 \
  to 1.00000e+01, screen 2.04358e-01

MAD_MAX = 1.50000e+01
set xrange [-3.21101e-01:1.03211e+01]
unset key
unset tics
unset border
plot x lc 'white'
