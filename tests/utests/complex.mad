local function unitCircle (r, n, val)
  local deg
  local z = {}
  if val ~= nil then
    for i,deg in ipairs(val) do
      z[i] = complex( r*cos( deg ), r*sin( deg ) )
    end
  else
    for i = 0,2*n do
      deg  = 2*pi*(i-1)/n
      z[i] = complex( r*cos( deg ), r*sin( deg ) )
    end
  end
  return z
end

