      assertNaN( complex( x  , nan):cosh().im )
      assertNaN( complex( nan, x  ):cosh().re )
      assertNaN( complex( nan, x  ):cosh().im )
    end
    assertEquals( complex( inf, 0):cosh(), complex( inf, 0) )
  end
