#== initialization =============================================================
if (!(exist("MAD_MIN") && exist("MAD_MAX"))) {
  MAD_MIN = 1000000;
  MAD_MAX = -1;
}
MAD_RESET_OBJECT = "unset for [i=MAD_MIN:MAD_MAX] object i"
MAD_RESET_LABEL  = "unset for [i=MAD_MIN:MAD_MAX] label i"
if (!(exist("MAD_OLD_TERM") && exist("MAD_NEW_TERM"))) {
  MAD_OLD_TERM = "";
  MAD_NEW_TERM = "";
}
MAD_SET_TERM = "if (!(MAD_OLD_TERM eq MAD_NEW_TERM)) { \
  @MAD_NEW_TERM; \
  MAD_OLD_TERM = MAD_NEW_TERM; \
}"
#===============================================================================

@MAD_SET_TERM
@MAD_RESET_OBJECT
@MAD_RESET_LABEL
set grid 
set border 15
set title 'testTableRange2' font ','
set key left top font ','
plot '-' using 1:2 with lines dashtype solid linewidth 1 linecolor 'red' title 'y1' axes x1y1,\

13 42
14 43
15 44
16 45
17 46
18 47
19 48
20 49
21 50
22 51
23 52
e
