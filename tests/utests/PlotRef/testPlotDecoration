#== initialization =============================================================
if (!(exist("MAD_MIN") && exist("MAD_MAX"))) {
  MAD_MIN = 1000000;
  MAD_MAX = -1;
}
MAD_RESET_OBJECT = "unset for [i=MAD_MIN:MAD_MAX] object i"
MAD_RESET_LABEL  = "unset for [i=MAD_MIN:MAD_MAX] label i"
if (!(exist("MAD_OLD_TERM") && exist("MAD_NEW_TERM"))) {
  MAD_OLD_TERM = "";
  MAD_NEW_TERM = "";
}
MAD_SET_TERM = "if (!(MAD_OLD_TERM eq MAD_NEW_TERM)) { \
  @MAD_NEW_TERM; \
  MAD_OLD_TERM = MAD_NEW_TERM; \
}"
#===============================================================================

@MAD_SET_TERM
@MAD_RESET_OBJECT
@MAD_RESET_LABEL
set border 3
set title 'testPlotDecoration' font 'Arial Black,21'
set key bottom right font 'Copperplate,11'
plot '-' using 1:2 with lines dashtype solid linewidth 1 linecolor 'red' title 'legend1 {/Symbol b} + β' axes x1y1,\
     ''  using 1:2 with lines dashtype solid linewidth 1 linecolor 'red' title 'legend2 {/Symbol B} + Β' axes x1y1,\

