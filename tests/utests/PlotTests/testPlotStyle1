#== initialization =============================================================
if (!(exist("MAD_MIN") && exist("MAD_MAX"))) {
  MAD_MIN = 1000000;
  MAD_MAX = -1;
}
MAD_RESET_OBJECT = "unset for [i=MAD_MIN:MAD_MAX] object i"
MAD_RESET_LABEL  = "unset for [i=MAD_MIN:MAD_MAX] label i"
if (!(exist("MAD_OLD_TERM") && exist("MAD_NEW_TERM"))) {
  MAD_OLD_TERM = "";
  MAD_NEW_TERM = "";
}
MAD_SET_TERM = "if (!(MAD_OLD_TERM eq MAD_NEW_TERM)) { \
  @MAD_NEW_TERM; \
  MAD_OLD_TERM = MAD_NEW_TERM; \
}"
#===============================================================================

@MAD_SET_TERM
@MAD_RESET_OBJECT
@MAD_RESET_LABEL
set grid 
set border 15
set title 'testPlotStyle1' font ','
set key left top font ','
set size 0.700000, 0.600000
set origin 0.300000, 0.400000
plot '-' using 1:2 with lines dashtype '..   ' linewidth 2.3 linecolor 'red' title 'y1' axes x1y1,\
     ''  using 1:2 with points pointsize 2 pointtype 2 linecolor 'blue' title 'y3' axes x1y1,\
     ''  using 1:2 with linespoints dashtype '--  ' linewidth 3.2 pointsize 4.1 pointtype 3 linecolor 'green' title 'y4' axes x1y1,\

